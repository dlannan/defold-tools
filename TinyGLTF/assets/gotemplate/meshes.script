local geom = require("assets.gotemplate.scripted_geom")
local sx = 0
function init(self)
	-- No factory resources are loaded when the factoryâ€™s parent
	-- collection is loaded. Calling create without having called
	-- load will create the resources synchronously.

	for i=0, 5 do 
		local pos = vmath.vector3(sx, 0, 0)
		self.go_id = factory.create("/fmesh#factory", pos)
		local url = msg.url(nil, self.go_id, "temp")
		print("URL:", url)
		geom:GenerateCube(url, 1, 1)
		sx = sx + 2
	end 
end

